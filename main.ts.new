        // é¿å…æ¨ç†è¿‡ç¨‹çš„å¥å­
        const reasoningIndicators = ['è®©æˆ‘', 'æˆ‘æ¥', 'åˆ†æ', 'é¦–å…ˆ', 'ç„¶å', 'æ¥ä¸‹æ¥', 'æ ¹æ®'];
        const hasReasoningIndicator = reasoningIndicators.some(indicator => 
            message.startsWith(indicator)
