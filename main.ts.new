        if (message.includes('\n')) {
            // ä¿æŒå¤šè¡Œæ ¼å¼ï¼Œä½†å¤„ç†æ¯è¡Œçš„ç‰¹æ®Šå­—ç¬¦
            return message
                .split('\n')
                .map(line => line.trim())
                .filter(line => line.length > 0)
                .map(line => {
                    // è½¬ä¹‰æ¯è¡Œä¸­çš„ç‰¹æ®Šå­—ç¬¦
                    return line
                        .replace(/"/g, '\\"')         // è½¬ä¹‰åŒå¼•å·
                        .replace(/\\/g, '\\\\')       // è½¬ä¹‰åæ–œæ   
                        .replace(/[\x00-\x1F\x7F]/g, '') // ç§»é™¤æ§åˆ¶å­—ç¬¦
                })
                .join('\n');
        } else {
            // å•è¡Œæ¶ˆæ¯çš„å¤„ç†æ–¹å¼
            let escaped = message
                .replace(/\s+/g, ' ')            // åˆå¹¶å¤šä½™ç©ºæ ¼
                .replace(/"/g, '\\"')            // è½¬ä¹‰åŒå¼•å·
                .replace(/\\/g, '\\\\')          // è½¬ä¹‰åæ–œæ 
                .replace(/[\x00-\x1F\x7F]/g, '') // ç§»é™¤æ§åˆ¶å­—ç¬¦
                .trim();
            // å¦‚æœæ¶ˆæ¯å¤ªé•¿ï¼Œæˆªæ–­å¹¶æ·»åŠ çœç•¥å·
            if (escaped.length > 500) {
                escaped = escaped.substring(0, 497) + '...';
            }
            // ç¡®ä¿æ¶ˆæ¯ä¸ä¸ºç©º
            if (!escaped) {
                escaped = 'æ›´æ–°æ–‡ä»¶å†…å®¹';
            }
            return escaped;
        }
    }
