    private isReasoningModelByResponse(choice: any): boolean {
        // æ£€æŸ¥æ˜¯å¦æœ‰reasoningç›¸å…³å­—æ®µï¼ˆOpenAI o1ç³»åˆ—ï¼‰
        if (choice.message && choice.message.reasoning) {
