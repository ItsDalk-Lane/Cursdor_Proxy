        // æ£€æŸ¥æ˜¯å¦åŒ…å«å¸¸è§çš„æäº¤åŠ¨è¯
        const commitVerbs = ['æ–°å¢', 'ä¿®æ”¹', 'åˆ é™¤', 'æ›´æ–°', 'é‡æ„', 'æ·»åŠ ', 'ç§»é™¤', 'ä¼˜åŒ–', 'ä¿®å¤'];
        const hasCommitVerb = commitVerbs.some(verb => message.includes(verb));
        if (hasCommitVerb && message.length > 10) {
            return true;
